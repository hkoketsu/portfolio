    "@mdx-js/react": "^1.5.3",
    "babel-plugin-styled-components": "^1.10.6",
    "classnames": "^2.2.6",
    "gatsby": "^2.18.17",
    "gatsby-cli": "^2.8.22",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-canonical-urls": "^2.1.18",
    "gatsby-plugin-emotion": "^4.1.18",
    "gatsby-plugin-mdx": "^1.0.64",
